defaults

# log to specified file
# (choose something that apache can write to)
logfile /tmp/gmail.log

# and not with syslog
# (apache will probabaly not have the right to write there)
syslog off

# dsn
dsn_notify off
dsn_return off

# account gmail
account gmail
host smtp.gmail.com
protocol smtp
auth on
user epinal.tsp@gmail.com
password epinalTSPcyb3r
tls on
tls_starttls off
tls_certcheck off
from epinal.tsp@gmail.com
maildomain gmail.com

# Set a default Account
account default : gmail
