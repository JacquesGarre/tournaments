<!DOCTYPE html>
<html>
    <head>
        <meta charset="UTF-8">
        <title>{% block title %}Welcome!{% endblock %}</title>
        {% block stylesheets %}
            {{ encore_entry_link_tags('jquery-ui-styles') }}
            {{ encore_entry_link_tags('app') }}
            {{ encore_entry_link_tags('global') }}
            {{ encore_entry_link_tags('home') }}
        {% endblock %}

    </head>
    <body>
        {% block body %}{% endblock %}
        {% block javascripts %}
            {{ encore_entry_script_tags('jquery') }}
            {{ encore_entry_script_tags('jquery-ui') }}
            {{ encore_entry_script_tags('bootstrap') }}
            {{ encore_entry_script_tags('app') }}
            {{ encore_entry_script_tags('players') }}
        {% endblock %}
    </body>
</html>
